<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="sample.Login">
   <children>
      <Button fx:id="loginbtn" layoutX="200.0" layoutY="292.0" mnemonicParsing="false" onAction="#handleGoLoginOnAction" prefHeight="32.0" prefWidth="76.0" text="Login" />
      <TextField fx:id="password" layoutX="200.0" layoutY="185.0" maxHeight="-Infinity" maxWidth="-Infinity" onAction="#handleGoLoginOnAction" prefHeight="40.0" prefWidth="200.0" />
      <TextField fx:id="username" layoutX="200.0" layoutY="122.0" maxHeight="-Infinity" maxWidth="-Infinity" onAction="#handleGoLoginOnAction" prefHeight="40.0" prefWidth="200.0" />
      <Button fx:id="registerbtn" layoutX="324.0" layoutY="292.0" mnemonicParsing="false" onAction="#handleGoRegisterOnAction" prefHeight="32.0" prefWidth="76.0" text="Register" />
      <Label layoutX="246.0" layoutY="65.0" text="Login">
         <font>
            <Font name="Courier New" size="36.0" />
         </font>
      </Label>
      <Label layoutX="113.0" layoutY="131.0" text="Username">
         <font>
            <Font name="Courier New" size="18.0" />
         </font>
      </Label>
      <Label layoutX="113.0" layoutY="194.0" text="Password">
         <font>
            <Font name="Courier New" size="18.0" />
         </font>
      </Label>
      <Label fx:id="noregist" layoutX="400.0" layoutY="131.0" opacity="0.0" text="User not regist" textFill="RED">
         <font>
            <Font name="Courier New" size="18.0" />
         </font>
      </Label>
      <Label fx:id="incorrectpassword" layoutX="400.0" layoutY="194.0" opacity="0.0" text="Incorrect password" textFill="RED">
         <font>
            <Font name="Courier New" size="18.0" />
         </font>
      </Label>
      <Label fx:id="noinput" layoutX="90.0" layoutY="248.0" opacity="0.0" text="Please enter your username and password" textFill="RED">
         <font>
            <Font name="Courier New" size="18.0" />
         </font>
      </Label>
   </children>
</AnchorPane>
